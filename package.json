{
  "name": "muzlist",
  "version": "1.0.0",
  "description": "SoundCloud client app",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV='production' ./node_modules/.bin/webpack -p --config webpack.config.js",
    "start": "webpack-dev-server --progress --hot",
    "test":
      "./node_modules/.bin/mocha --require babel-register ./test/setup.js --reporter spec src/**/*.spec.js",
    "precommit": "lint-staged",
    "format": "prettier --write 'src/*.js' 'src/**/*.js'",
    "lint": "eslint 'src/*.js' 'src/**/*.js' || true"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alxisk/muzlist.git"
  },
  "author": "alxisk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alxisk/muzlist/issues"
  },
  "homepage": "https://github.com/alxisk/muzlist#readme",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-input-range": "^1.2.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-saga": "^0.15.6",
    "regenerator-runtime": "^0.10.5",
    "soundcloud": "^3.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7"
  }
}
