{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "plugins": [
    "jsx-a11y",
    "import",
    "react",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/media-has-caption": "off",
    "react/jsx-filename-extension": "off",
    "jsx-a11y/click-events-have-key-events": "off"
  }
}
